@base <http://stat.stadt-zuerich.ch/> .
@prefix stat: <http://ld.stadt-zuerich.ch/statistics/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .

<WebSite> a schema:WebSite ;
    schema:headline "Statistisches Informationsportal" ;
    schema:hasPart <http://stat.stadt-zuerich.ch/> .

<http://stat.stadt-zuerich.ch/> a schema:WebPage ;
    schema:headline "Statistisches Informationsportal" ;
    schema:name "Einstiegsseite" ;
    schema:description "Die Statistik Stadt Zürich ist das Zahlengedächtnis der Stadtverwaltung. [...]" ;
    schema:hasPart <Navigation> .

<Navigation> a schema:SiteNavigationElement ;
    schema:hasPart <Bevoelkerung> ;
    schema:hasPart <Wirtschaft> ;
    schema:hasPart <Bauen&Wohnen> .

<Bevoelkerung> a schema:WebPage ;
    schema:headline "Bevoelkerung" ;
    schema:hasPart <Bevoelkerung/Navigation> ;
    schema:hasPart <Bevoelkerung/Thema1> ;
    schema:hasPart <Bevoelkerung#Thema2> .

<Bevoelkerung/Navigation> a schema:SiteNavigationElement ;
    schema:hasPart <Bevoelkerung/Bestand> ;
    schema:hasPart <Bevoelkerung/Bilanz> ;
    schema:hasPart <Bevoelkerung/Szenarian> ;
    schema:hasPart <Bevoelkerung/Haushalte> ;
    schema:hasPart <Bevoelkerung/Heirat&Scheidung> ;
    schema:hasPart <Bevoelkerung/Geburt&Sterbefaelle> .

<Bevoelkerung/Thema1> a schema:WebPageElement ;
    schema:name "Bevoelkerung, 1901 - 1934" ;
    schema:hasPart stat:Bevoelkerung .

<Bevoelkerung#Thema2> a schema:WebPageElement ;
    schema:name "Beliebteste Vorname bei Neugeborenen" ;
    schema:hasPart stat:Vornamen .
