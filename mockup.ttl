@base <http://stat.stadt-zuerich.ch/> .
@prefix stat: <http://ld.stadt-zuerich.ch/statistics/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .

<#WebSite> a schema:WebSite ;
    schema:headline "Statistisches Informationsportal" ;
    schema:hasPart <http://stat.stadt-zuerich.ch/> .

<http://stat.stadt-zuerich.ch/> a schema:WebPage ;
    schema:headline "Statistisches Informationsportal" ;
    schema:name "Einstiegsseite" ;
    schema:description "Die Statistik Stadt Zürich ist das Zahlengedächtnis der Stadtverwaltung. [....]" ;
    schema:hasPart <#Navigation> .

<#Navigation> a schema:SiteNavigationElement ;
    schema:hasPart <Bevölkerung> ;
    schema:hasPart <Wirtschaft> ;
    schema:hasPart <Bauen&Wohnen> .

<Bevölkerung> a schema:WebPage ;
    schema:headline "Bevölkerung" ;
    schema:hasPart <Bevölkerung#Navigation> ;
    schema:hasPart <Bevölkerung#Thema1> ;
    schema:hasPart <Bevölkerung#Thema2> .

<Bevölkerung#Navigation> a schema:SiteNavigationElement ;
    schema:hasPart <Bevölkerung/Bestand> ;
    schema:hasPart <Bevölkerung/Bilanz> ;
    schema:hasPart <Bevölkerung/Szenarian> ;
    schema:hasPart <Bevölkerung/Haushalte> ;
    schema:hasPart <Bevölkerung/Heirat&Scheidung> ;
    schema:hasPart <Bevölkerung/Geburt&Sterbefaelle> .

<Bevölkerung#Thema1> a schema:WebPageElement ;
    schema:name "Bevölkerung, 1901 - 1934" ;
    schema:hasPart stat:Bevölkerung .

<Bevölkerung#Thema2> a schema:WebPageElement ;
    schema:name "Beliebteste Vorname bei Neugeborenen" ;
    schema:hasPart stat:Vornamen .
