PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX qb: <http://purl.org/linked-data/cube#>
PREFIX ssz-schema: <http://ld.stadt-zuerich.ch/schema/>
PREFIX ssz-dimension: <http://ld.stadt-zuerich.ch/statistics/property/>

SELECT DISTINCT ?view ?label ?dimension ?lock ?dataset WHERE {
  GRAPH <https://linked.opendata.swiss/graph/zh/statistics> {

  ?view a qb:SliceKey ;
    rdfs:label ?label ;
    qb:componentProperty ?dimensionLock  ;
    ssz-schema:viewStructure/qb:component/qb:dimension ?dimension .
  
  ?sliceUri a qb:Slice ;
    qb:sliceStructure ?view .

  OPTIONAL {
    ?sliceUri ?dimension ?lock .
  }
}}