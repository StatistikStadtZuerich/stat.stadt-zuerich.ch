PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX qb: <http://purl.org/linked-data/cube#>
PREFIX ex: <http://example.org/>
PREFIX sh: <http://www.w3.org/ns/shacl#>
PREFIX ssz-schema: <http://ld.stadt-zuerich.ch/schema/>
PREFIX ssz-dimension: <http://ld.stadt-zuerich.ch/statistics/property/>

SELECT ?property ?notation ?datatype WHERE {
  GRAPH <https://linked.opendata.swiss/graph/zh/statistics> {
    {
      ?property a qb:MeasureProperty ;
        rdfs:range ?datatype ;
        skos:notation ?notation .
    } UNION {
      ?property a qb:DimensionProperty ;
        skos:notation ?notation .
      MINUS {
        ?property a qb:MeasureProperty .
      }
    }
  }
} ORDER BY ?property
